    "fs-extra": "^7.0.1",
    "node-emoji": "^1.10.0",
    "node-sketch": "^0.12.2",
    "shelljs": "^0.8.3",
    "svgo": "^1.1.1",
    "terminal-link": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5",
    "eslint-config-ebay": "^1.0.0",
    "mocha": "^6.0.2",
